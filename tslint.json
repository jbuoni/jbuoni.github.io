{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {
		"no-unused-expression": true
	},
	"rules": {
		"no-console": false,
		"quotemark": [
			true,
			"single"
		],
		"member-access": [
			false
		],
		"ordered-imports": [
			false
		],
		"max-line-length": [
			false
		],
		"member-ordering": [
			false
		],
		"array-type": [
			false
		],
		"no-empty": false,
		"arrow-parens": false,
		"object-literal-sort-keys": false,
		"max-classes-per-file": false,
		"one-variable-per-declaration": [
			false
		],
		"trailing-comma": false,
		"forin": false,
		"object-literal-key-quotes": false,
		"indent": [
			true,
			"tabs",
			4
		],
		"typedef": [
			true,
			"call-signature"
		],
		"no-duplicate-imports": true,
		"no-unused-variable": true
	},
	"rulesDirectory": []
}